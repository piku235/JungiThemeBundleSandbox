<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="jungi_environment_index" pattern="/">
        <default key="_controller">JungiEnvironmentBundle:Default:index</default>
    </route>
    <route id="jungi_environment_theme_manage" pattern="/theme/manage">
        <default key="_controller">JungiEnvironmentBundle:Theme:manage</default>
    </route>
    <route id="jungi_environment_admin" pattern="/admin">
        <default key="_controller">JungiEnvironmentBundle:Admin:index</default>
    </route>
    <route id="jungi_environment_admin_settings" pattern="/admin/settings">
        <default key="_controller">JungiEnvironmentBundle:Admin:settings</default>
    </route>
</routes>
